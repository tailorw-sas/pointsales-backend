            <artifactId>jakarta.annotation-api</artifactId>
        </dependency>
    </dependencies>

    <distributionManagement>
        <repository>
            <id>github</id>
            <name>GitHub OWNER Apache Maven Packages</name>
            <url>https://maven.pkg.github.com/tailorw-sas/pointsales-backend</url>
        </repository>
    </distributionManagement>
</project>
